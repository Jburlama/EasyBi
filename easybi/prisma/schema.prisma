generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Integration {
  id        String   @id @default(cuid())
  name      String
  phone     String
  cnpj      String
  email     String
  logo      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
